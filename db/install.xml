<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/online_users_map/db" VERSION="20180501" COMMENT="XMLDB file for Moodle online_users_map block"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="block_online_users_map" COMMENT="Lat Lng coordinates for users">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" />
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" />
      	<FIELD NAME="lat" TYPE="float" LENGTH="15" NOTNULL="true" UNSIGNED="false" DEFAULT="0.0000000000" SEQUENCE="false" ENUM="false" DECIMALS="12" />
      	<FIELD NAME="lng" TYPE="float" LENGTH="15" NOTNULL="true" UNSIGNED="false" DEFAULT="0.0000000000" SEQUENCE="false" ENUM="false" DECIMALS="12" />
      	<FIELD NAME="city" TYPE="char" LENGTH="40" NOTNULL="false" DEFAULT="" SEQUENCE="false"/>
      	<FIELD NAME="country" TYPE="char" LENGTH="2" NOTNULL="false" DEFAULT="" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="Primary key for block_online_users_map" />
        <KEY NAME="userid" TYPE="foreign" FIELDS="userid" REFTABLE="user" REFFIELDS="id" />
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>